<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.net.atos.uk.TravelDashboard.Dashboard.Main.DashboardRootController">
   <left>
      <AnchorPane fx:id="apMenu" prefHeight="800.0" prefWidth="80.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="uploadInMenu" layoutX="11.0" layoutY="89.0" mnemonicParsing="false" onAction="#handleUploadCentre" text="Upload" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="250.0" />
            <Button fx:id="reportInMenu" layoutX="11.0" layoutY="178.0" mnemonicParsing="false" onAction="#handleReportCentre" text="Report" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="400.0" />
            <Button fx:id="exitInMenu" layoutX="21.0" layoutY="502.0" mnemonicParsing="false" onAction="#handleLogout" text="Exit" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="550.0" />
            <Button fx:id="mainInMenu" layoutX="11.0" layoutY="89.0" mnemonicParsing="false" onAction="#handleMainDashboard" text="Main" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="100.0" />
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" top="10.0" />
         </BorderPane.margin>
      </AnchorPane>
   </left>
</BorderPane>
